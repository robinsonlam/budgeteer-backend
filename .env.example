# Database Configuration
MONGODB_URI="mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=AppName"

# JWT Configuration
JWT_SECRET="your_jwt_secret_key_here_make_it_long_and_random"
JWT_EXPIRES_IN=2880s # 48 Hours

# MongoDB MCP API Configuration
MDB_MCP_API_CLIENT_ID="your_mcp_api_client_id"
MDB_MCP_API_CLIENT_SECRET="your_mcp_api_client_secret"

# Node Configuration
NODE_VERSION=22.18.0

# Google OAuth (if using Google authentication)
GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"

# Server Configuration
PORT=3000
NODE_ENV=development
